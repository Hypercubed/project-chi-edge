    "overrides": {
      "github:HubSpot/tether@1.3.2": {
        "main": "js/tether",
        "directories": {
          "lib": "dist"
        },
        "meta": {
          "js/tether.js": {
            "exports": "Tether",
            "format": "global"
          }
        }
      },
      "github:angular-ui/bower-ui-grid@3.1.1": {
        "main": "ui-grid"
      },
      "github:angular/bower-angular-animate@1.5.5": {
        "main": "angular-animate",
        "registry": "jspm",
        "dependencies": {
          "angular": "^1.3.0"
        },
        "meta": {
          "angular-animate.js": {
            "deps": [
              "angular"
            ],
            "format": "global"
          }
        }
      },
      "github:angular/bower-angular-cookies@1.5.5": {
        "main": "angular-cookies",
        "registry": "jspm",
        "dependencies": {
          "angular": "^1.3.0-rc.1"
        },
        "meta": {
          "angular-cookies.js": {
            "deps": [
              "angular"
            ],
            "format": "global"
          }
        }
      },
      "github:angular/bower-angular-route@1.5.5": {
        "main": "angular-route",
        "registry": "jspm",
        "dependencies": {
          "angular": "^1.3.0"
        },
        "meta": {
          "angular-route.js": {
            "deps": [
              "angular"
            ],
            "format": "global"
          }
        }
      },
      "github:angular/bower-angular-sanitize@1.5.5": {
        "main": "angular-sanitize",
        "registry": "jspm",
        "dependencies": {
          "angular": "^1.3.0"
        },
        "meta": {
          "angular-sanitize.js": {
            "deps": [
              "angular"
            ],
            "format": "global"
          }
        }
      },
      "github:angular/bower-angular@1.5.5": {
        "main": "angular",
        "meta": {
          "angular.js": {
            "exports": "angular",
            "format": "global"
          }
        }
      },
      "github:codemirror/codemirror@5.14.2": {
        "format": "global",
        "directories": {
          "lib": "."
        }
      },
      "github:twbs/bootstrap@4.0.0-alpha.2": {
        "files": null,
        "dependencies": {
          "jquery": "*",
          "tether": "github:HubSpot/tether@^1.1.1"
        },
        "meta": {
          "js/bootstrap.js": {
            "deps": [
              "jquery",
              "tether"
            ],
            "exports": "$",
            "format": "global"
          }
        }
      },
      "npm:babel-runtime@5.8.38": {
        "main": false,
        "dependencies": {},
        "optionalDependencies": {
          "core-js": "^1.2.0"
        }
      },
      "npm:browserify-zlib@0.1.4": {
        "dependencies": {
          "readable-stream": "^2.0.2",
          "pako": "~0.2.0"
        },
        "map": {
          "_stream_transform": "readable-stream/transform"
        }
      },
      "npm:d3@3.5.17": {
        "meta": {
          "d3.js": {
            "exports": "d3",
            "format": "global"
          }
        }
      },
      "npm:font-awesome@4.6.1": {
        "main": "css/font-awesome.css!",
        "jspmNodeConversion": false,
        "registry": "jspm",
        "dependencies": {
          "css": "0.1"
        }
      },
      "npm:inherits@2.0.1": {
        "ignore": [
          "test.js"
        ]
      },
      "npm:jquery@2.2.3": {
        "format": "amd"
      }
    }
  },
  "xo": {
    "esnext": true,
    "space": 2,
    "envs": [
      "node",
      "browser"
    ],
    "ignores": [
      "app/jspm_packages/**",
      "app/system.config.js",
      "*.js",
      "dataset/**",
      "!dataset/example/**"
    ],
    "rules": {
      "space-before-function-paren": [
        2,
        {
          "anonymous": "always",
          "named": "always"
        }
      ],
      "no-warning-comments": 0
    }
  }
}
